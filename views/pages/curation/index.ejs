<%- include('../header') -%>

<div id="" class="ui three column grid">
    <% for(var i=0; i < uncurated.length; i++) { %>
        <div class="column">
            <%- include('curation-card', {uncurated: uncurated[i]}); %>
        </div>
    <% } %>
</div>

<div id="success-toast" class="ui success message">
    <i class="close icon"></i>
    <div class="header">
      Your user registration was successful.
    </div>
    <p>You may now log-in with the username you have chosen</p>
  </div>

  <div id="error-toast" class="ui negative message">
    <i class="close icon"></i>
    <div class="header">
      We're sorry we can't apply that discount
    </div>
    <p>That offer has expired
  </p></div>
<%- include('../footer') -%>
